<h4>Add data source </h4>
<form role="form" action="/projects/<%= project._id  %>/adddatasource" method="post">
  <div class="form-group">
    <label for="email">Name:</label>
    <input type="text" class="form-control" id="name" name="name" value="datasource1">
  </div>
  <div class="form-group">
    <label for="pwd">Description:</label>
    <input type="text" placeholder="optional" class="form-control" id="description" name="description">
  </div>




  <div class="form-group ">
    <label class="control-label " for="select">
       Data source type:
      </label>
    <select class="select form-control" id="datasource_type" name="datasource_type">
       <option value="test_signal">
        Generated signal (zolware)
       </option>
        <option value="other">
        other
       </option>
      </select>
  </div>

  <div id="test_signals">


